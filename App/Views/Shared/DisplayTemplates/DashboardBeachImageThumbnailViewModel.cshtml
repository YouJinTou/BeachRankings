@model DashboardBeachImageThumbnailViewModel

<div class="dashboard-image-wrapper img-container">
    <a href="@Model.Path" data-lightbox="beach-images">
        <img class="img-responsive" src="@Model.Path" alt="Image" />
    </a>
    <span data-remove-image="@Model.Id" class="a-like-span glyphicon glyphicon-floppy-remove"></span>
</div>